{
	"name": "tcltk",
	"description": "Bindings to Tcl/Tk for the D programming language",
	"homepage": "https://github.com/nomad-software/tcltk",
	"authors": ["Gary Willoughby"],
	"copyright": "Copyright (c) 2014 Gary Willoughby",
	"license": "MIT",
	"libs-posix": ["tcl", "tk"],
	"excludedSourceFiles": [
		"source/c/*",
	],
	"dependencies": {
		"x11": "~master"
	},
	"subConfigurations": {
		"x11": "tcltk-import"
	},
	"configurations": [
		{
			"name": "library",
			"targetType": "library",
			"sourceFiles-windows-x86-dmd": ["lib/windows/x86/omf/standalone/tcl86t.lib", "lib/windows/x86/omf/standalone/tk86t.lib"],
			"sourceFiles-windows-x86-ldc": ["lib/windows/x86/coff/standalone/tcl86t.lib", "lib/windows/x86/coff/standalone/tk86t.lib"],
			"sourceFiles-windows-x86-gnu": ["lib/windows/x86/coff/standalone/tcl86t.lib", "lib/windows/x86/coff/standalone/tk86t.lib"],
			"sourceFiles-windows-x86_64": ["lib/windows/x86_64/coff/standalone/tcl86t.lib", "lib/windows/x86_64/coff/standalone/tk86t.lib"],
			"copyFiles-windows-x86": ["dist/x86/tcl86t.dll", "dist/x86/tk86t.dll"],
			"copyFiles-windows-x86_64": ["dist/x86_64/tcl86t.dll", "dist/x86_64/tk86t.dll"],
			"targetPath": "build/lib",
		},
		{
			"name": "activetcl",
			"targetType": "library",
			"sourceFiles-windows-x86-dmd": ["lib/windows/x86/omf/activetcl/tcl86.lib", "lib/windows/x86/omf/activetcl/tk86.lib"],
			"sourceFiles-windows-x86-ldc": ["lib/windows/x86/coff/activetcl/tcl86.lib", "lib/windows/x86/coff/activetcl/tk86.lib"],
			"sourceFiles-windows-x86-gnu": ["lib/windows/x86/coff/activetcl/tcl86.lib", "lib/windows/x86/coff/activetcl/tk86.lib"],
			"sourceFiles-windows-x86_64": ["lib/windows/x86_64/coff/activetcl/tcl86.lib", "lib/windows/x86_64/coff/activetcl/tk86.lib"],
			"targetPath": "build/lib",
		},
	],
}
